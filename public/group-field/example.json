					"drop": "lazy",
					"required": true,
					"fields": [
						{
							"required": true,
							"suffix": "EUR",
							"align": "right"
						},
						{
							"type": "checkbox",
							"required": true
						},
						{
							"type": "select",
							"options": [
								"1",
								"2",
								"3"
							],
							"required": true
						}
					]
				},
				{
					"type": "info",
					"name": "lazyinfo",
					"text": "='current value: ' + lazy"
				}
			]
		}
	]
