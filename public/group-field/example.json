{
	"processors": [
		{
			"url": "/"
		}
	],
	"sequence": [
		{
			"name": "form",
			"label": "Grouping Fields",
			"description": "This form demonstrates use of grouping fields aggregating a set of subordinated fields.",
			"fields": [
				{
					"name": "eager",
					"label": "Eager Labelled Group",
					"type": "group",
					"required": true,
					"fields": [
						{
							"label": "text",
							"required": true
						},
						{
							"label": "checkbox",
